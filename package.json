{
  "name": "next-typesafe-url",
  "description": "JSON serializable, fully typesafe, and zod validated URL search params, dynamic route params, and routing for NextJS",
  "license": "MIT",
  "version": "1.1.0",
  "author": "Ethan Niser",
  "repository": {
    "type": "git",
    "url": "https://github.com/ethanniser/next-typesafe-url"
  },
  "keywords": [
    "next",
    "nextjs",
    "urlSearchParams",
    "typesafe",
    "typescript",
    "zod"
  ],
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": "dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/cli.ts src/index.ts --format cjs,esm --dts",
    "lint": "tsc",
    "release": "npm run build && changeset version && changeset publish"
  },
  "dependencies": {
    "meow": "9.0.0",
    "chokidar": "^3.5.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.0",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "@types/node": "^18.15.0",
    "@types/react": "^18.0.28",
    "next": "^13.2.3",
    "react": "18.2.0",
    "zod": "^3.20.2"
  },
  "peerDependencies": {
    "next": "^13.2.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "zod": "^3.20.2"
  }
}
